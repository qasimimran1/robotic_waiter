<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="myTestRobot">
	<xacro:include filename="$(find cyton_gamma_1500_description)/cyton_gamma_1500_macros.urdf.xacro" />
	<link name="fixed_base">
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.54"																																																																		/>																																																																																																																																																																																																																																																																																																																																																																																													
			<geometry>
				<box size="0.34 0.31 1.0"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.54"/>
			<geometry>
				<box size="0.34 0.31 1.0"/>
			</geometry>
		</collision>
		
		<inertial>
			<origin rpy="0 0 0" xyz="0.0 0.0 0.5"/>
			<mass value="12"/>
			<!-- inretia tensor for solid cuboid: m=12, h=1.12, d=0.49, w=0.41 -->
			<inertia ixx="1.4225" ixy="0" ixz="0" iyy="1.4945" iyz="0" izz="0.4082"/>
		</inertial>
 
	</link>
	
	<joint name="right_arm_joint" type="fixed">
		<parent link="fixed_base"/>
		<child link="right_arm_base_link"/>
		<!--origin rpy="0 0 0" xyz="0 0.25 0.05"/-->
		<origin rpy="1.5708 -1.5708 0" xyz="0.067 -0.162 0.8"/>
	</joint>
	

	
	
	<xacro:cyton_gamma_1500 prefix="right_arm_" virtual_ee="true" />
  
	<joint name="left_arm_joint" type="fixed">
		<parent link="fixed_base"/>
		<child link="left_arm_base_link"/>
		<!--origin rpy="0 0 0" xyz="0 -0.3 0.05"/-->
		<origin rpy="-1.5708 1.5708 0" xyz="0.067 0.162 0.8"/>
	</joint>
	
	
	<xacro:cyton_gamma_1500 prefix="left_arm_" virtual_ee="true" />
	
	
	
	<joint name="wheel_left_joint" type="continuous">
    <parent link="fixed_base"/>
    <child link="wheel_left"/>
    <origin rpy="1.570796 0 0" xyz="0.0 0.18 0.095"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_left">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
      <mass value="0.5"/>
      <!-- inretia tensor for solid cylinder: m=0.5, h=0.05, r=0.095 -->
      <inertia ixx="1.232291667e-03" ixy="0" ixz="0" iyy="1.232291667e-03" iyz="0" izz="2.25625e-03"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.095"/>
      </geometry>
      <material name="blackish">
        <color rgba="0.2 0.2 0.2 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.095"/>
      </geometry>
    </collision>
  </link>
  
  
  <joint name="wheel_right_joint" type="continuous">
    <parent link="fixed_base"/>
    <child link="wheel_right"/>
    <origin rpy="1.570796 0 0" xyz="0.0 -0.18 0.095"/>
    <axis xyz="0 0 1"/>
  </joint>
  <link name="wheel_right">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
      <mass value="0.5"/>
      <!-- inretia tensor for solid cylinder: m=0.5, h=0.05, r=0.095 -->
      <inertia ixx="1.232291667e-03" ixy="0" ixz="0" iyy="1.232291667e-03" iyz="0" izz="2.25625e-03"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.095"/>
      </geometry>
      <material name="blackish"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.095"/>
      </geometry>
    </collision>
  </link>
  
</robot>
