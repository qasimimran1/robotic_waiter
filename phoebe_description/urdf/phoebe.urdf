      <inertia ixx="8.0167e-05" ixy="0" ixz="0" iyy="8.0167e-05" iyz="0" izz="5.4167e-05"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
