<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Repeat num_cycles="2">            
                <Sequence>
                    <Action ID="LookAtTheTray"/>
                    <RetryUntilSuccesful num_attempts="3">
                        <Timeout msec="300">
                            <Action ID="DetectHandles"/>
                        </Timeout>
                    </RetryUntilSuccesful>                     
                    <Action ID="LookUp"/>                    
                </Sequence>            
        </Repeat>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="lookAtTheTray"/>
        <Action ID="DetectHandles"/>
        <Action ID="LookUp"/>             
        
    </TreeNodesModel>
    <!-- ////////// -->
</root>
